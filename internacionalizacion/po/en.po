#: calculadora.c:14
#, c-format
msgid "Selecciona una operación:\n"
msgstr "Select an operation:\n"

#: calculadora.c:15
#, c-format
msgid "1. Suma\n"
msgstr "1. Add\n"

#: calculadora.c:16
#, c-format
msgid "2. Resta\n"
msgstr "2. Subtract\n"

#: calculadora.c:17
#, c-format
msgid "3. Multiplicación\n"
msgstr "3. Multiply\n"

#: calculadora.c:18
#, c-format
msgid "4. División\n"
msgstr "4. Divide\n"

#: calculadora.c:19
#, c-format
msgid "5. Salir\n"
msgstr "5. Exit\n"

#: calculadora.c:29
#, c-format
msgid "Introduce tu opción: "
msgstr "Enter your option: "

#: calculadora.c:31 calculadora.c:40 calculadora.c:46
#, c-format
msgid "Entrada no válida. Por favor, introduce un número.\n"
msgstr "Invalid input. Please enter a number.\n"

#: calculadora.c:38
#, c-format
msgid "Introduce el primer número: "
msgstr "Enter the first number: "

#: calculadora.c:44
#, c-format
msgid "Introduce el segundo número: "
msgstr "Enter the second number: "

#: calculadora.c:53 calculadora.c:56 calculadora.c:59 calculadora.c:63
#, c-format
msgid "Resultado: %.2f\n"
msgstr "Result: %.2f\n"

#: calculadora.c:65
#, c-format
msgid "Error: No se puede dividir por cero.\n"
msgstr "Error: Cannot divide by zero.\n"
